<?xml version="1.0" encoding="utf-8"?>
<InteractionList>
  <Interactions>
    <Interaction>
      <Name>leave_tutorial_room</Name>
      <Type>Click</Type>
      <ALLTags>tutorialComplete</ALLTags>
      <NONETags>tutorialStarted</NONETags>
      <TakeTags>tutorialComplete</TakeTags>
      <SpecialActions>ExitTutorial</SpecialActions>
      <MaxDistance>10</MaxDistance>
      <TooFar>go_to_door</TooFar>
    </Interaction>
    <Interaction>
      <Name>go_to_door</Name>
      <Type>Derivative</Type>
      <SpecialActions>ComeHere</SpecialActions>
      <Next>leave_tutorial_room</Next>
    </Interaction>
  </Interactions>
</InteractionList>
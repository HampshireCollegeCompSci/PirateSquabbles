<?xml version="1.0" encoding="utf-8"?>
<InteractionList>
  <Interactions>
    <Interaction>
      <Name>rings_click_get_colors</Name>
      <Type>Click</Type>
      <ALLTags>intro|prism_puzzle_started</ALLTags>
      <Text>(You take the colors)</Text>
      <Image>red_and_blue_rings</Image>
      <GiveItems>blue_circle|red_circle</GiveItems>
      <SpecialActions>Destroy</SpecialActions>
    </Interaction>
    <Interaction>
      <Name>rings_click_dont_get</Name>
      <Type>Click</Type>
      <ALLTags>intro|prism_puzzle_started</ALLTags>
      <Text>(Nevermind)</Text>
      <Image>red_and_blue_rings</Image>
    </Interaction>
    <Interaction>
      <Name>rings_click_prism_puzzle_not_started</Name>
      <Type>Click</Type>
      <NONETags>prism_puzzle_started</NONETags>
      <Text>They're two colored rings.</Text>
      <Image>red_and_blue_rings</Image>
    </Interaction>
    <Interaction>
      <Name>DefaultTooFar</Name>
      <Type>Derivative</Type>
      <Text>Maybe I should go closer...</Text>
    </Interaction>
    <Interaction>
      <Name>DefaultCannotUse</Name>
      <Type>Derivative</Type>
      <Text>I cannot use this item now.</Text>
      <Image></Image>
      <SpecialActions>ReturnSelected</SpecialActions>
    </Interaction>
  </Interactions>
</InteractionList>